{
  "name": "model-mixes",
  "version": "0.0.11",
  "author": "Catena Labs <dev@catena.xyz>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "build": "pnpm clean && tsup ./mix-definitions/index.ts",
    "check": "pnpm format:check && pnpm lint && pnpm type-check && pnpm test",
    "clean": "git clean -xdf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "prepublishOnly": "pnpm check && pnpm build",
    "test": "vitest",
    "type-check": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vitest": "^2.0.5"
  }
}
